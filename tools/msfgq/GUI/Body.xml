<xpp:cdata xmlns="http://www.w3.org/1999/xhtml" xmlns:xpp="http://epeios.q37.info/ns/xpp" xmlns:xdh="http://epeios.q37.info/ns/xdhtml">
  <fieldset>
    <span style="display: flex; ">
      <fieldset style="display: flex; align-items: center; justify-content: space-around">
        <legend xdh:onevents="Test">Midi in</legend>
        <select id="MidiIn">
          <option value="dummy">Placeholder</option>
        </select>
        <input xdh:onevent="Refresh" type="image" style="width:20px; height: 20px;" src="data:image/png;base64," />
      </fieldset>
      <fieldset class="neat">
        <legend>Key</legend>
        <select id="AccidentalAmount" xdh:onevent="SetAccidentalAmount">
          <option value="0">0</option>
          <option value="1">1</option>
          <option value="2">2</option>
          <option value="3">3</option>
          <option value="4">4</option>
          <option value="5">5</option>
          <option value="6">6</option>
          <option value="7">7</option>
        </select>
        <span id="Accidental" xdh:onevent="SetAccidental" xdh:radio="Accidental">
          <label>
            <input type="radio" name="Accidental" value="Sharp" />
            <span>♯</span>
          </label>
          <label>
            <input type="radio" name="Accidental" value="Flat" />
            <span>♭</span>
          </label>
        </span>
      </fieldset>
      <fieldset class="neat">
        <legend>Time</legend>
        <input id="Numerator" type="number" max="12" min="1" value="4" size="2" />
        <span>/</span>
        <select id="Denominator">
          <option value="1">1</option>
          <option value="2">2</option>
          <option value="4" selected="selected">4</option>
          <option value="8">8</option>
          <option value="16">16</option>
        </select>
      </fieldset>
      <fieldset class="neat">
        <legend>Octave</legend>
        <input id="Octave" type="number" size="1" min="0" max="9" value="4" />
      </fieldset>
    </span>
    <fieldset style="display: flex; flex-direction: column; gap: 5px">
      <legend>Score</legend>
      <fieldset>
        <legend>Preview</legend>
        <div id="Score"></div>
      </fieldset>
      <span style="display: flex">
        <fieldset style="display: table; box-sizing: border-box; border: solid 1px;">
          <legend>Duration/rest</legend>
          <span id="1" xdh:onevent="Duration" class="duration">
            <input type="image" src="data:image/png;base64," />
          </span>
          <span class="duration">
            <input id="2" xdh:onevent="Duration" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration">
            <input id="3" xdh:onevent="Duration" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration">
            <input id="4" xdh:onevent="Duration" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration">
            <input id="5" xdh:onevent="Duration" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration">
            <input id="6" xdh:onevent="Duration" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration" style="border-right:solid 1px!important;">
            <input id="7" xdh:onevent="Duration" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration" style="border: none;"></span>
          <span class="duration" style="border-right:solid 1px!important;">
            <input xdh:onevent="Rest" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration" style="border: none;"></span>
          <span class="duration">
            <input xdh:onevent="Dot" type="image" src="data:image/png;base64," />
          </span>
          <span class="duration" style="border-right:solid 1px!important;">
            <input xdh:onevent="Tie" type="image" src="data:image/png;base64," />
          </span>
        </fieldset>
        <!--fieldset style="display: flex; align-items: center;">
          <legend>Clef</legend>
          <label class="clef" xdh:onevent="treble" style="margin: 0px 3px;">
            <input name="clef" type="radio" />
            <img src="data:image/png;base64," alt="" />
          </label>
          <label class="clef" style="margin: 0px 3px;">
            <input name="clef" type="radio"/>
            <img src="data:image/png;base64," alt=""/>
          </label>
          <label class="clef" xdh:onevent="bass">
            <input name="clef" type="radio" />
            <img src="data:image/png;base64," alt="" >
          </label>
        </fieldset/-->
      </span>
      <fieldset style="display: flex; align-items: center; gap:5px;">
        <legend>Cursor</legend>
        <fieldset xdh:radio="Cursor" xdh:onevent="Cursor">
          <label>
            <input name="Cursor" type="radio" value="Insert" checked="checked">Insert</input>
          </label>
          <label>
            <input name="Cursor" type="radio" value="Overwrite">Overwrite</input>
          </label>
        </fieldset>
        <button xdh:onevent="Append">Append</button>
        <button xdh:onevent="Suppr">Suppr.</button>
        <button xdh:onevent="Clear">Clear all</button>
      </fieldset>
    </fieldset>
    <fieldset>
      <legend>Audio</legend>
      <div id="audio"></div>
    </fieldset>  
    <fieldset id="Scripts">
      <legend>Scripts</legend>
    </fieldset>
  </fieldset>
  <fieldset>
    <div class='piano-container'>
      <ul id="beautiful-piano">
        <li>
          <div id="k0" xdh:onevent="Keyboard" class="anchor F3"></div>
        </li>
        <li>
          <div id="k2" xdh:onevent="Keyboard" class="anchor G3"></div>
          <span id="k1" xdh:onevent="Keyboard" class="Fs3 Gb3"></span>
        </li>
        <li>
          <div id="k4" xdh:onevent="Keyboard" class="anchor A3"></div>
          <span id="k3" xdh:onevent="Keyboard"  class="Gs3 Ab3"></span>
        </li>
        <li>
          <div id="k6" xdh:onevent="Keyboard" class="anchor B3"></div>
          <span id="k5" xdh:onevent="Keyboard" class="As3 Bb3"></span>
        </li>
        <li>
          <div id="k7" xdh:onevent="Keyboard" class="anchor C4"></div>
        </li>
        <li>
          <div id="k9" xdh:onevent="Keyboard" class="anchor D4"></div>
          <span id="k8" xdh:onevent="Keyboard" class="Cs4 Db4"></span>
        </li>
        <li>
          <div id="k11" xdh:onevent="Keyboard" class="anchor E4"></div>
          <span id="k10" xdh:onevent="Keyboard" class="Ds4 Eb4"></span>
        </li>
        <li>
          <div id="k12" xdh:onevent="Keyboard" class="anchor F4"></div>
        </li>
        <li>
          <div id="k14" xdh:onevent="Keyboard" class="anchor G4"></div>
          <span id="k13" xdh:onevent="Keyboard" class="Fs4 Gb4"></span>
        </li>
        <li>
          <div id="k16" xdh:onevent="Keyboard" class="anchor A4"></div>
          <span id="k15" xdh:onevent="Keyboard" class="Gs4 Ab4"></span>
        </li>
        <li>
          <div id="k18" xdh:onevent="Keyboard" class="anchor B4"></div>
          <span id="k17" xdh:onevent="Keyboard" class="As4 Bb4"></span>
        </li>
        <li>
          <div id="k19" xdh:onevent="Keyboard" class="anchor C5"></div>
        </li>
        <li>
          <div id="k21" xdh:onevent="Keyboard" class="anchor D5"></div>
          <span id="k20" xdh:onevent="Keyboard" class="Cs5 Db5"></span>
        </li>
        <li>
          <div id="k23" xdh:onevent="Keyboard" class="anchor E5"></div>
          <span id="k22" xdh:onevent="Keyboard" class="Ds5 Eb5"></span>
        </li>
        <li>
          <div id="k24" xdh:onevent="Keyboard" class="anchor F5"></div>
        </li>
        <li>
          <div id="k26" xdh:onevent="Keyboard" class="anchor G5"></div>
          <span id="k25" xdh:onevent="Keyboard" class="Fs5 Gb5"></span>
        </li>
        <li>
          <div id="k28" xdh:onevent="Keyboard" class="anchor A5"></div>
          <span id="k27" xdh:onevent="Keyboard" class="Gs5 Ab5"></span>
        </li>
        <li>
          <div id="k30" xdh:onevent="Keyboard" class="anchor B5"></div>
          <span id="k29" xdh:onevent="Keyboard" class="As5 Bb5"></span>
        </li>
        <li>
          <div id="k31" xdh:onevent="Keyboard" class="anchor C6"></div>
        </li>
      </ul>
    </div>
  </fieldset>
  <fieldset>
    <details>
      <Summary>Output</Summary>
      <fieldset id="Output"/>
    </details>
  </fieldset>
</xpp:cdata>