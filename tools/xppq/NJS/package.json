{
 "name": "xppq",
 "version": "0.1.3",
 "author": "Claude SIMON (http://q37.info/contact/)",
 "description": "XML parser and preprocessor.",
 "main": "xppq.js",
 "engines": {
  "node": ">=6"
 },
 "binary": {
  "module_name" : "xppqnjs",
  "module_path" : "./lib/{platform}-{arch}/",
