  "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
  "host" : "https://q37.info"
 },
 "dependencies"  : {
   "node-pre-gyp": "0.6.x"
 },
 "devDependencies": {
  "aws-sdk": "2.x"
 },
 "scripts": {
  "preinstall": "npm install node-pre-gyp",
  "install": "node-pre-gyp install --fallback-to-build",
  "test": "node demo.js"
},
 "repository" : {
  "type" : "git",
  "url" : "https://github.com/epeios-q37/node-xppq.git"
  },
